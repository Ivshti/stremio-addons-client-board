html, body
	margin 0
	padding 0
	font-family Arial, Helvetica, Sans-Serif
	
ul, li
	list-style none
	
.boardRow
	ul
		white-space nowrap
	li 
		background-size cover
		width 15%
		display inline-block
				
		&:before 
			content: "";
			display: block;
			padding-top: 146.4%
	
	.square li
		&:before
			padding-top: 100%
	
	.landscape li
		&:before
			padding-top: 56.25%
	